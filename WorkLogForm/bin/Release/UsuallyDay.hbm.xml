<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="ClassLibrary" assembly="ClassLibrary">

  <class name="UsuallyDay" table="LOG_T_USUALLYDAY" >
    <id name="Id" column="id" type="long"  length="10" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="Name" column="NAME" type="String" length="10"/>
    <property name="StartTime" column="STARTTIME" type="long" length="10"/>
    <property name="WorkTimeStart" column="WorkTimeStart" type="long" length="10"/>
    <property name="WorkTimeEnd" column="WorkTimeEnd" type="long" length="10"/>
    <property name="WorkDay" column="WORKDAY" type="String" length="7" />
    <property name="State" column="STATE" type="int" length="1"/>
    <property name="TimeStamp" column="TIMESTAMP" type="long" length="10"/>
  </class>

</hibernate-mapping>